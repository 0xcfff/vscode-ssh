{
    "name": "ssh",
    "displayName": "ssh",
    "description": "ssh integration",
    "version": "0.0.1",
    "publisher": "chrmarti",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Languages"
    ],
    "activationEvents": [
        "onLanguage:ssh_config",
        "onCommand:ssh.run"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "ssh_config",
                "aliases": [
                    "ssh config",
                    "ssh_config"
                ],
                "filenamePatterns": [
                    "**/.ssh/config"
                ],
                "filenames": [
                    "ssh.config"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ssh_config",
                "scopeName": "source.config.ssh",
                "path": "./syntaxes/ssh_config.tmLanguage.json"
            }
        ],
        "commands": [
            {
                "command": "ssh.run",
                "title": "Run ssh"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/node": "^7.0.13",
        "tslint": "^5.1.0",
        "typescript": "^2.3.1",
        "vscode": "^1.0.0"
    }
}